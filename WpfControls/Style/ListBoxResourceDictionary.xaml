<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfControls">
    <DataTemplate x:Key="datePickerViewDataTemplate">
        <local:DatePickerControlView></local:DatePickerControlView>
    </DataTemplate>
    <DataTemplate x:Key="stringViewDataTemplate">
        <local:StringControlView></local:StringControlView>
    </DataTemplate>
    <DataTemplate x:Key="groupBoxViewDataTemplate">
        <local:GroupBoxControlView></local:GroupBoxControlView>
    </DataTemplate>
    <DataTemplate x:Key="enumViewDataTemplate">
        <local:EnumControlView></local:EnumControlView>
    </DataTemplate>
    <DataTemplate x:Key="bitEnumViewDataTemplate">
        <local:BitEnumControlView></local:BitEnumControlView>
    </DataTemplate>
    <local:YDataTemplateSelector x:Key="YDataTemplateSelectorDictionary" 
                                DatePickerViewDataTemplate="{StaticResource datePickerViewDataTemplate}"
                                StringViewDataTemplate="{StaticResource stringViewDataTemplate}"
                                GroupBoxViewDataTemplate="{StaticResource groupBoxViewDataTemplate}"
                        EnumViewDataTemplate="{StaticResource enumViewDataTemplate}"
                        BitEnumViewDataTemplate="{StaticResource bitEnumViewDataTemplate}"/>
</ResourceDictionary>