<UserControl x:Class="WpfControls.GroupBoxControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfControls"
             mc:Ignorable="d">
    <Grid>
        <GroupBox Header="{Binding Path=Label}">
            <ListView Grid.Column="1" Grid.Row="1" ItemsSource="{Binding ListViewItemModels}" ItemTemplateSelector="{StaticResource YDataTemplateSelectorDictionary}" />
        </GroupBox>
    </Grid>
</UserControl>